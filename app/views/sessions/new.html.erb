<div class="span4">
	<div class="well" >
		<h1>Login here</h1><br/>
		<%= form_tag 'login', method: 'post' do %>
			<%= label_tag :name %>
		    <%= text_field_tag 'name', nil, :id => "username" %>
		    <%= label_tag :password %>
		    <%= password_field_tag 'password', nil, :id => "password" %>
		    <br/><br/>
		    <%= submit_tag 'Login' %>
		<% end %> 
	</div>
	<%= render 'shared/breadcrumbhomenarrow' %>
</div>
